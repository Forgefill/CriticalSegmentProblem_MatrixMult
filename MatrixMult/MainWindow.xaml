<Window x:Class="MatrixMult.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MatrixMult"
        mc:Ignorable="d"
        Title="MainWindow" Height="467" Width="800">
    <Grid>
        <Grid x:Name="A" Margin="10,10,400,217" Background="#FFEEEEEE"/>
        <Grid x:Name="B" Margin="405,10,10,217" Background="#FFD7D7D7"/>
        <Button x:Name="SizeSet" Content="Set n,m, k" HorizontalAlignment="Left" Margin="10,286,0,0" VerticalAlignment="Top" Height="18" Width="63" Click="SizeSet_Click"/>
        <TextBox x:Name="N" HorizontalAlignment="Left" Margin="10,263,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="54"/>
        <TextBox x:Name="M" HorizontalAlignment="Left" Margin="69,263,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="54" TextInput="TextBox_TextInput"/>
        <Button x:Name="Mult" Content="Multiply" HorizontalAlignment="Left" Margin="78,286,0,0" VerticalAlignment="Top" Width="96" Height="18" Click="Mult_Click"/>
        <TextBox x:Name="K" HorizontalAlignment="Left" Margin="128,263,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="73"/>
        <Grid x:Name="C" Margin="405,239,10,10" Background="#FFD4D4D4"/>

    </Grid>
</Window>
